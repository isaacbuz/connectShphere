{
  "name": "connectsphere",
  "version": "1.0.0",
  "description": "Decentralized AI-powered social platform with autonomous agents",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:perf": "jest --testMatch='**/*.perf.test.js'",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:agents\"",
    "dev:backend": "nodemon src/api/server.ts",
    "dev:frontend": "cd src/frontend && vite",
    "dev:agents": "python src/agents/main.py",
    "build": "npm run build:frontend && npm run build:backend && npm run compile",
    "build:frontend": "cd src/frontend && vite build",
    "build:backend": "tsc -p tsconfig.json",
    "start": "NODE_ENV=production node dist/api/server.js",
    "start:dev": "nodemon src/api/server.ts",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:testnet": "hardhat run scripts/deploy.js --network mumbai",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "k8s:deploy": "kubectl apply -f infrastructure/k8s/",
    "agents:start": "python src/agents/main.py",
    "blockchain:test": "hardhat test",
    "blockchain:deploy": "hardhat run scripts/deploy.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "docs": "typedoc --out docs src",
    "clean": "rm -rf dist build node_modules coverage artifacts cache",
    "precommit": "lint-staged",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "ethers": "^6.8.0",
    "web3": "^4.1.2",
    "ipfs-http-client": "^60.0.1",
    "socket.io": "^4.6.2",
    "redis": "^4.6.10",
    "bullmq": "^4.12.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "axios": "^1.5.1",
    "openai": "^4.14.0",
    "@pinecone-database/pinecone": "^1.1.0",
    "langchain": "^0.0.180",
    "kafkajs": "^2.2.4",
    "@grpc/grpc-js": "^1.9.7",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.1",
    "morgan": "^1.10.0",
    "ioredis": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-toastify": "^9.1.3",
    "@web3-react/core": "^8.2.3",
    "@web3-react/injected-connector": "^6.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "eslint": "^8.51.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.3",
    "hardhat": "^2.18.1",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "typescript": "^5.2.2",
    "concurrently": "^8.2.1",
    "vite": "^4.5.0",
    "@vitejs/plugin-react": "^4.1.0",
    "typedoc": "^0.25.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
} 